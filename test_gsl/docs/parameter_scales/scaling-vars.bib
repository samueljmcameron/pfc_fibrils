@article{Cameron:2018kq,
author = {Cameron, Samuel and Kreplak, Laurent and Rutenberg, Andrew D},
title = {{Polymorphism of stable collagen fibrils}},
journal = {Soft Matter},
year = {2018},
volume = {14},
number = {23},
pages = {4772--4783},
publisher = {The Royal Society of Chemistry},
doi = {10.1039/C8SM00377G},
language = {English},
read = {Yes},
rating = {0},
date-added = {2018-09-13T15:53:12GMT},
date-modified = {2018-12-03T13:51:26GMT},
abstract = {Collagen fibrils are versatile self-assembled structures that provide mechanical integrity within mammalian tissues. The radius of collagen fibrils vary widely depending on experimental conditions in vitro or anatomical location in vivo. Here we explore the variety of thermodynamically stable fibril configurations that are available. We use a liquid crystal model of radial collagen fibril structure with a double-twist director field. Using a numerical relaxation method we show that two dimensionless parameters, the ratio of saddle-splay to twist elastic constants k24/K22 and the ratio of surface tension to chiral strength $\equiv$ $\gamma$/(K22q), largely specify both the scaled fibril radius and the associated surface twist of equilibrium fibrils. We find that collagen fibrils are the stable phase with respect to the cholesteric phase only when the reduced surface tension is small, â‰² 0.2. Within this stable regime, collagen fibrils can access a wide range of radii and associated surface twists. Remarkably, we find a maximal equilibrium surface twist of 0.33 rad (19{\textdegree}). Our results are compatible with corneal collagen fibrils, and we show how the large surface twist can explain the narrow distribution of corneal fibril radii. Conversely, we show how small surface twist is required for the thermodynamic stability of tendon fibrils in the face of considerable polydispersity of radius.},
url = {http://xlink.rsc.org/?DOI=C8SM00377G},
local-url = {file://localhost/Users/samcameron/Documents/papers-library/Library.papers3/Articles/2018/Cameron/Soft%20Matter%202018%20Cameron.pdf},
file = {{Soft Matter 2018 Cameron.pdf:/Users/samcameron/Documents/papers-library/Library.papers3/Articles/2018/Cameron/Soft Matter 2018 Cameron.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1039/C8SM00377G}}
}

